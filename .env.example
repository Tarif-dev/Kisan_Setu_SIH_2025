# AgriAssist Environment Configuration
# Copy this file to .env and fill in your API keys

# Gemini AI API Key for voice assistant and image analysis
EXPO_PUBLIC_GEMINI_API_KEY=your_gemini_api_key_here

# OpenWeather API Key for weather data
EXPO_PUBLIC_OPENWEATHER_API_KEY=your_openweather_api_key_here

# App Configuration
EXPO_PUBLIC_APP_NAME=AgriAssist
EXPO_PUBLIC_APP_VERSION=1.0.0

# Development Configuration
EXPO_PUBLIC_ENVIRONMENT=development

# API Base URLs (optional, defaults are provided)
# EXPO_PUBLIC_GEMINI_API_URL=https://generativelanguage.googleapis.com/v1beta
# EXPO_PUBLIC_OPENWEATHER_API_URL=https://api.openweathermap.org/data/2.5

# Features Toggle (optional)
EXPO_PUBLIC_ENABLE_VOICE_ASSISTANT=true
EXPO_PUBLIC_ENABLE_PUSH_NOTIFICATIONS=true
EXPO_PUBLIC_ENABLE_OFFLINE_MODE=true
